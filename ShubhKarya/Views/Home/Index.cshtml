        width: 100%;
    }


    .bestSeller img {
        min-width: 100%;
        height: 200px;
        object-fit: cover;
    }

    .auto-input {
        background: radial-gradient( circle farthest-corner at 10% 20%, rgba(255,94,247,1) 17.8%, rgba(2,245,255,1) 100.2% );
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
    }

