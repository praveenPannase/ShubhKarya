        <!-- Modal -->
        <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h1  class="modal-title fs-5" id="staticBackdropLabel">Restration Form</h1>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">

                        <div class="card-body row">
                            <div class="col-sm-12">
                                <input type="text" style="max-width:100%" class="form-control p-4 mb-3" id="name" placeholder="Full Name" required="required" data-validation-required-message="Please enter your name" />
                            </div>
                            <div class="col-sm-6">
                                <input type="email" style="max-width:100%" class="form-control p-4" id="email" placeholder="Your Email" required="required" data-validation-required-message="Please enter your email" />
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="col-sm-6">
                                <input type="text" style="max-width:100%" class="form-control p-4" id="name" placeholder="Mobile No" required="required" data-validation-required-message="Please enter your name" />
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="col-sm-6">
                                <input type="date" style="max-width:100%" class="form-control p-4" id="name" required="required" data-validation-required-message="Please enter your name" />
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="col-sm-6">
                                <input type="file" style="max-width:100%" class="form-control p-4" id="name" required="required" data-validation-required-message="Please enter your name" />
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="col-sm-6">
                                <input type="text" style="max-width:100%" class="form-control p-4" id="name" placeholder="Applied Position" required="required" data-validation-required-message="Please enter your name" />
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="col-sm-6">
                                <input type="text" style="max-width:100%" class="form-control p-4" id="name" placeholder="Expected Salary" required="required" data-validation-required-message="Please enter your name" />
                                <p class="help-block text-danger"></p>
                            </div>

                            <div class="col-sm-12">
                                <textarea style="max-width:100%" class="form-control p-4" id="name" required="required" data-validation-required-message="Please enter your name" placeholder="Please enter your Address"></textarea>
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="d-flex justify-content-evenly">
                                <button class="btn btn-primary py-2 px-5" type="submit" id="sendMessageButton">Submit</button>
                                <button class="btn btn-primary py-2 px-5" type="reset" id="sendMessageButton">Reset</button>
                            </div>
                        </div>


                    </div>
                 
                </div>
            </div>
        </div>
    </div>

</div>

